{
  "name": "sketch",
  "version": "1.0.0",
  "description": "ThreeJS, WebGL sketches",
  "main": "index.js",
  "license": "Creative Commons Attribution-NonCommercial-ShareAlike 4.0 International License",
  "author": {
    "name": "vico",
    "url": "https://github.com/star8ks"
  },
  "dependencies": {
    "three": "^0.86.0",
    "three-orbit-viewer": "^69.3.1"
  },
  "devDependencies": {
    "babel-preset-env": "^1.6.1",
    "babelify": "^7.3.0",
    "browserify": "^14.0.0",
    "budo": "^10.0.4",
    "glslify": "^6.0.1",
    "uglify-js": "^3.0.26"
  },
  "scripts": {
    "start": "budo index.js:bundle.js -v --live",
    "startssl": "budo index.js:dist/bundle.js -v -d --live --ssl --cert=../../server.cer --key=../../server.key",
    "build": "browserify.cmd index.js | uglifyjs -cm > bundle.js"
  },
  "private": true,
  "keywords": [
    "three",
    "js",
    "threejs",
    "glsl"
  ],
  "browserify": {
    "transform": [
      "babelify",
      "glslify"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/star8ks/sketch.git"
  },
  "homepage": "https://github.com/star8ks/sketch"
}
